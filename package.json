{
  "name": "storacha-www",
  "type": "module",
  "version": "0.20.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "preview-static": "nuxi generate && npx serve dist",
    "preview-cf": "nuxi build --preset=cloudflare-pages && npx wrangler pages dev dist --port 3000",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "clean": "nuxi cleanup --force && pnpm i --force",
    "release": "pnpx bumpp --commit --push --tag"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.15",
    "@iconify-json/simple-icons": "^1.2.62",
    "@iconify/json": "^2.2.417",
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.13.1",
    "@nuxtjs/fontaine": "^0.5.0",
    "@nuxtjs/plausible": "^1.2.0",
    "@nuxtjs/seo": "^3.3.0",
    "@unhead/vue": "^2.0.19",
    "@unocss/nuxt": "^66.2.3",
    "@unocss/preset-icons": "^0.63.6",
    "@vueuse/core": "^11.3.0",
    "@vueuse/nuxt": "^13.9.0",
    "defu": "^6.1.4",
    "fast-xml-parser": "^4.5.3",
    "radix-vue": "^1.9.17",
    "unocss": "^0.63.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@nuxt/eslint": "^1.12.1",
    "@unocss/eslint-config": "^0.63.6",
    "eslint": "^9.39.2",
    "nuxt": "^3.17.5",
    "typescript": "^5.9.3",
    "vue-tsc": "^2.2.12",
    "wrangler": "^3.114.15"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}