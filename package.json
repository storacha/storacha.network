{
  "name": "storacha-www",
  "type": "module",
  "version": "0.20.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "preview-static": "nuxi generate && npx serve dist",
    "preview-cf": "nuxi build --preset=cloudflare-pages && npx wrangler pages dev dist --port 3000",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "clean": "nuxi cleanup --force && pnpm i --force",
    "release": "pnpx bumpp --commit --push --tag"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/simple-icons": "^1.2.10",
    "@iconify/json": "^2.2.351",
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.8",
    "@nuxtjs/fontaine": "^0.5.0",
    "@nuxtjs/plausible": "^1.2.0",
    "@nuxtjs/seo": "^3.0.3",
    "@unhead/vue": "^2.0.10",
    "@unocss/nuxt": "^66.2.3",
    "@unocss/preset-icons": "^0.63.6",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^13.4.0",
    "defu": "^6.1.4",
    "fast-xml-parser": "^4.5.0",
    "radix-vue": "^1.9.8",
    "unocss": "^0.63.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@nuxt/eslint": "^1.4.1",
    "@unocss/eslint-config": "^0.63.6",
    "eslint": "^9.13.0",
    "nuxt": "^3.17.5",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.1.10",
    "wrangler": "^3.84.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}